
public class HelloWorld {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		System.out.println("*     *");
		System.out.println("*     *");
		lineStars();
		space()
		lineStars();
	}
	
	public static void lineStars() {
		System.out.print("*******");
	}
	
	public static void space() {
		System.out.println(" ");
	}
}
